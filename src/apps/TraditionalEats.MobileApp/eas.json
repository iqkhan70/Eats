{
  "cli": {
    "version": ">= 12.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_ENV": "ip",
        "EXPO_DOCTOR_SKIP_DEPENDENCY_VERSION_CHECK": "true"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_ENV": "staging",
        "EXPO_DOCTOR_SKIP_DEPENDENCY_VERSION_CHECK": "true"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_ENV": "production",
        "EXPO_DOCTOR_SKIP_DEPENDENCY_VERSION_CHECK": "true"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {}
    }
  }
}
